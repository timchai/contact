<h1>Contacts</h1>


<table>
<tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Phone</th>
</tr>
<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.first_name %></td>
    <td><%= contact.last_name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.phone %></td>
  </tr>
<% end %>
</table>

<%=@new_contact_fn%>
